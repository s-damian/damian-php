{
    "name": "s-damian/damian-php",
    "type": "project",
    "description": "The Damian PHP Framework.",
    "keywords": ["framework", "damian-php", "DamianPhp"],
    "license": "MIT",
    "authors": [
        {
            "name": "Stephen Damian",
            "email": "contact@devandweb.fr",
            "homepage": "https://github.com/s-damian"
        }
    ],
    "require": {
        "php": ">=8.1",
        "s-damian/damian-php-fw": "^1.0",
        "vlucas/phpdotenv": "5.3.*",
        "swiftmailer/swiftmailer": "6.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.5.*",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "autoload": {
        "psr-4" : {
            "App\\": "app/"
        }
    },
    "scripts": {
        "test": [
            "php8.1 ./vendor/phpunit/phpunit/phpunit"
        ],
        "test-class": [
            "php8.1 ./vendor/phpunit/phpunit/phpunit --filter"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
